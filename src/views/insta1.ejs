    <section class="header__section">
        <div class="container">
         <header class="header">
           <h2 class="header__head"><i class='bx bxl-instagram-alt'></i> Instagram </h2>
           <div class="input__box">
               <input class="header__input" type="text" placeholder="found">
           </div>
             <div class="header__imgs">
               <a href="http://localhost:2000/"><i class='bx bxs-home'></i></a>
               <i class='bx bx-message-rounded-dots img2'></i>
               <i class='bx bxs-plus-square'></i>
               <i class='bx bx-shape-circle'></i>
               <i class='bx bx-heart-circle'></i>
               <img class="odil" src="http://instagram-newapp.herokuapp.com/img/odil.jpg" width="30" alt="">
             </div>
         </header>
       </div>
       
   </section> <!-- end of section -->
   
    <header>

        <div class="container">
    
            <div class="profile">
    
                <div class="profile-image">
    
                    <img src="http://instagram-newapp.herokuapp.com/img/odil.jpg" width="200" alt="">
    
                </div>   
                
                <div class="profile-user-settings">
    
                    <h1 class="profile-user-name"><%= findUsers.username %></h1>
    
                    <button class="btn profile-edit-btn">Edit Profile</button>
    
                    <button class="btn profile-settings-btn" aria-label="profile settings"><i class="fas fa-cog" aria-hidden="true"></i></button>
    
                </div>
    
                <div class="profile-stats">
    
                    <ul>
                        <li><span class="profile-stat-count"><%= findUsers.postCount %></span> posts</li>
                        <li><span class="profile-stat-count"><%= findUsers.followers %></span> followers</li>
                        <li><span class="profile-stat-count"><%= findUsers.following %></span> following</li>
                    </ul>
    
                </div>
    
                <div class="profile-bio">
    
                    <p><span class="profile-real-name"><%= findUsers.username %> :</span> <%= findUsers.desc %>üí™‚òùÔ∏è</p>
    
                </div>
            </div>
            <!-- End of profile section -->
    
